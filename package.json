{
  "private": true,
  "description": "This is a Monorepo root package which is used to execute build scripts.",
  "engines": {
    "node": ">=14.17"
  },
  "scripts": {
    "---start---": "",
    "start:core": "nx run elements:start",
    "start:storybook": "nx run-many --target=start --projects=storybook,elements --output-style=stream",
    "start:angular": "nx run-many --target=start --projects=,elements,elements-angular --output-style=stream",
    "start:angular-example": "nx run angular-example:start",
    "start:react-example": "nx run-many --target=start --projects=elements-react*,elements --output-style=stream",
    "start:react-example:only": "nx run react-example:start",
    "start:vue-example": "nx run-many --target=start --projects=elements-vue*,elements --output-style=stream",
    "start:vue-example:only": "nx run vue-example:start",
    "start:landingpage": "nx run-many --target=start --projects=landingpage,elements-vue,elements --output-style=stream",
    "start:landingpage:only": "nx run landingpage:start",
    "start:landingpage-new": "nx run-many --target=start --projects=landingpage-new,elements-react,elements --output-style=stream",
    "start:landingpage-new:only": "nx run landingpage-new:start",
    "---build---": "",
    "build": "nx run-many --target=build --output-style=stream",
    "build:core": "nx run elements:build",
    "build:core:dev": "nx run elements:build:dev",
    "build:storybook": "nx run storybook:build",
    "build:angular": "nx run elements-angular:build",
    "build:react": "nx run elements-react:build",
    "build:vue": "nx run elements-vue:build",
    "build:angular-example": "nx run elements-angular-example:build",
    "build:react-example": "nx run elements-react-example:build",
    "build:vue-example": "nx run elements-vue-example:build",
    "build:landingpage": "nx run landingpage:build",
    "build:landingpage-new": "nx run landingpage-new:build",
    "---test---": "",
    "test": "nx run-many --target=test --output-style=stream --configuration=ci",
    "test:core": "nx run elements:test",
    "test:core:watch": "nx run elements:test:watch",
    "---lint---": "",
    "lint": "nx run-many --target=lint --output-style=stream",
    "lint:fix": "nx run-many --target=lint --configuration=fix --output-style=stream",
    "lint:core": "nx run elements:lint",
    "lint:storybook": "nx run storybook:lint",
    "lint:angular": "nx run elements-angular:lint",
    "lint:react": "nx run elements-react:lint",
    "lint:vue": "nx run elements-vue:lint",
    "---misc---": "",
    "clean": "nx run-many --target=clean && shx rm -rf dist",
    "clean:node-modules": "nx run-many --target=clean --configuration=nodejs && shx rm -rf node_modules",
    "---lerna---": "",
    "changelog": "yarn changelog:root && yarn changelog:pkgs",
    "changelog:root": "standard-version",
    "changelog:pkgs": "lerna exec --concurrency 1 --stream 'standard-version'",
    "format": "yarn prettier --write '**/*.{js,jsx,ts,tsx,md,html,css,scss}'",
    "new:component": "node scripts/newComponent.js",
    "prepare:release": "lerna version --conventional-commits --no-changelog --yes --no-git-tag-version",
    "publish:yalc": "node ./scripts/publishYalc.js",
    "publish:canary": "yarn lerna publish --no-verify-access --canary --exact --preid next.$(git rev-parse --short HEAD) --force-publish",
    "publish:npm": "yarn lerna publish --conventional-commits --preid test.$(git rev-parse --short HEAD)",
    "version": "node scripts/bumprootversion.js && yarn changelog",
    "postversion": "git add -A"
  },
  "devDependencies": {
    "@nrwl/storybook": "15.5.2",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/react-output-target": "^0.3.1",
    "@stencil/sass": "1.5.2",
    "@stencil/vue-output-target": "^0.6.0",
    "@types/css-font-loading-module": "^0.0.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "26.0.14",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.2",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-react": "^7.32.1",
    "fs-extra": "^9.0.0",
    "jest": "26.4.2",
    "jest-cli": "26.4.2",
    "js-yaml": "^3.14.0",
    "lerna": "^5.5.2",
    "lint-staged": "^10.5.3",
    "nx": "15.5.2",
    "puppeteer": "^10.4.0",
    "prettier": "^2.1.2",
    "prompts": "^2.4.0",
    "shelljs": "^0.8.4",
    "shx": "^0.3.2",
    "standard-version": "^8.0.0",
    "svgo": "^2.8.0",
    "ts-node": "^10.9.1",
    "tslint-config-prettier": "^1.18.0",
    "yalc": "^1.0.0-pre.32"
  },
  "dependencies": {
    "@hedgedoc/markdown-it-task-lists": "^1.0.3",
    "@material/button": "^13.0.0",
    "@material/card": "^13.0.0",
    "@material/checkbox": "^13.0.0",
    "@material/chips": "^13.0.0",
    "@material/data-table": "^13.0.0",
    "@material/dialog": "^13.0.0",
    "@material/drawer": "^13.0.0",
    "@material/fab": "^13.0.0",
    "@material/form-field": "^13.0.0",
    "@material/icon-button": "^13.0.0",
    "@material/image-list": "^13.0.0",
    "@material/linear-progress": "^13.0.0",
    "@material/list": "^13.0.0",
    "@material/menu": "^13.0.0",
    "@material/radio": "^13.0.0",
    "@material/ripple": "^13.0.0",
    "@material/select": "^13.0.0",
    "@material/slider": "^13.0.0",
    "@material/snackbar": "^13.0.0",
    "@material/switch": "^13.0.0",
    "@material/tab": "^13.0.0",
    "@material/tab-bar": "^13.0.0",
    "@material/tab-indicator": "^13.0.0",
    "@material/tab-scroller": "^13.0.0",
    "@material/textfield": "^13.0.0",
    "@material/typography": "^13.0.0",
    "@stencil/core": "2.17.1",
    "@tarekraafat/autocomplete.js": "^10.2.7",
    "@tiptap/core": "^2.0.0-beta.159",
    "@tiptap/extension-link": "^2.0.0-beta.33",
    "@tiptap/extension-task-item": "^2.0.0-beta.36",
    "@tiptap/extension-task-list": "^2.0.0-beta.29",
    "@tiptap/starter-kit": "^2.0.0-beta.164",
    "autosize": "^4.0.2",
    "date-fns": "^2.16.1",
    "flatpickr": "^4.6.13",
    "prosemirror-markdown": "^1.6.0",
    "tippy.js": "^6.2.3"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/elements-angular/elements"
    ],
    "nohoist": [
      "**/@types/jasmine",
      "**/@types/jasminewd2",
      "**/@types/eslint",
      "**/@types/estree",
      "**/@types/eslint-scope",
      "**/flatpickr",
      "**/@mdx-js/*"
    ]
  },
  "name": "core",
  "version": "8.0.0"
}

