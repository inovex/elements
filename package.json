{
  "name": "core",
  "version": "9.3.1",
  "private": true,
  "description": "This is a Monorepo root package which is used to execute build scripts.",
  "packageManager": "pnpm@10.29.2",
  "engines": {
    "node": ">=24",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "start:storybook": "turbo start --filter=@inovex.de/elements-storybook --filter=@inovex.de/elements",
    "start:landingpage": "turbo start --filter=@inovex.de/landingpage",
    "start:ui-patterns-playground": "turbo start --filter=@inovex.de/ui-patterns --filter=@inovex.de/ui-patterns-playground",
    "build": "turbo build",
    "test": "turbo test --filter=@inovex.de/elements --filter=@inovex.de/elements-storybook",
    "test:core:watch": "turbo run test:watch --filter=@inovex.de/elements",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "lint:ci": "turbo lint",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "qc": "pnpm lint:ci && pnpm format && pnpm build && pnpm test",
    "clean": "turbo clean && shx rm -rf node_modules .turbo",
    "changeset": "changeset",
    "release-packages": "pnpm build && changeset publish",
    "release-packages:canary": "pnpm build && changeset version --snapshot canary && changeset publish --tag canary",
    "version-packages": "changeset version && pnpm install --lockfile-only"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/css-font-loading-module": "^0.0.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "29.5.11",
    "@types/node": "18.16.9",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "autoprefixer": "^10.4.16",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "fs-extra": "^9.0.0",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.5.3",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "prompts": "^2.4.0",
    "shelljs": "^0.8.4",
    "shx": "^0.3.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "turbo": "^2.8.5",
    "typescript": "~5.3.2",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-static-copy": "^0.17.0",
    "yalc": "^1.0.0-pre.32"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
