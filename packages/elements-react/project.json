{
  "name": "elements-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/elements-react/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run elements-react:compile"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx watch --projects=elements-react -- nx run elements-react:compile"
      }
    },
    "compile": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/elements-react",
        "command": "tsc --project ./tsconfig.lib.json"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "shx rm -rf packages/elements-react/dist"
      },
      "configurations": {
        "nodejs": {
          "command": "shx rm -rf packages/elements-react/node_modules"
        }
      }
    }
  }
}
