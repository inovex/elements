    "lint:fix": "eslint src --fix",
    "test": "stencil test --e2e --ci",
    "test:e2e": "stencil test --e2e",
    "test:e2e-update": "stencil test --e2e -u ./",
    "test:e2e-watch": "stencil test --e2e --watch",
    "test:spec": "stencil test --spec",
    "test:spec-watch": "stencil test --spec --watch",
    "icon:integrate-icons": "node scripts/integrateIcons.js",
    "icon:optimize-icons": "svgo -f ./src/assets/ino-icon --config ../../svgo.config.js"
  },
