{
  "name": "elements",
  "sourceRoot": "packages/elements/src",
  "projectType": "library",
  "generators": {
    "@nxext/stencil:component": {
      "style": "scss"
    }
  },
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nxext/stencil:test",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "packages/elements/tsconfig.lib.json",
        "configPath": "packages/elements/stencil.config.ts",
        "outputPath": "packages/elements"
      }
    },
    "build": {
      "executor": "@nxext/stencil:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "packages/elements/tsconfig.lib.json",
        "configPath": "packages/elements/stencil.config.ts",
        "outputPath": "packages/elements/",
        "ci": true,
        "dev": false,
        "prod": true,
        "verbose": true,
        "watch": false
      },
      "configurations": {
        "dev": {
          "ci": true,
          "dev": true,
          "prod": false,
          "watch": false,
          "verbose": true
        }
      }
    },
    "start": {
      "executor": "@nxext/stencil:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "packages/elements/tsconfig.lib.json",
        "configPath": "packages/elements/stencil.config.ts",
        "outputPath": "dist/elements",
        "ci": false,
        "dev": true,
        "prod": false,
        "watch": true,
        "verbose": true
      }
    },
    "e2e": {
      "executor": "@nxext/stencil:e2e",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "packages/elements/tsconfig.lib.json",
        "configPath": "packages/elements/stencil.config.ts",
        "outputPath": "dist/elements"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": "packages/elements/**/*.{ts,tsx}"
      }
    },
    "icons": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node --project packages/elements/tsconfig.json packages/elements/scripts/integrateIcons.ts"
      },
      "configurations": {
        "optimize": {
          "command": "svgo -f packages/elements/src/assets/ino-icon --config svgo.config.js"
        }
      }
    }
  }
}
