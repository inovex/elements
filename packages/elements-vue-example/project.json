{
  "name": "elements-vue-example",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/elements-vue-example/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/elements-vue-example",
        "command": "vite build"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/elements-vue-example"
      },
      "defaultConfiguration": "dev",
      "configurations": {
        "dev": {
          "command": "vite serve"
        },
        "prod": {
          "command": "vite serve --mode=production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/elements-vue-example/src/**/*.{ts,tsx,vue,js}"
        ]
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/elements-vue-example",
        "command": "shx rm -rf dist"
      },
      "configurations": {
        "nodejs": {
          "command": "shx rm -rf node_modules"
        }
      }
    }
  }
}
