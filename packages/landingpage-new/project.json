{
  "name": "landingpage-new",
  "sourceRoot": "packages/landingpage-new/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "outputs": ["{projectRoot}/dist"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/landingpage-new",
        "commands": ["next build", "next export"],
        "parallel": false,
        "color": true
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/landingpage-new"
      },
      "defaultConfiguration": "dev",
      "configurations": {
        "dev": {
          "command": "next dev -p 4600"
        },
        "prod": {
          "command": "next start -p 4600"
        }
      },
      "dependsOn": ["^dev"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/landingpage-new",
        "command": "next lint"
      },
      "configurations": {
        "fix": {
          "command": "next lint --fix"
        }
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/landingpage-new",
        "commands": ["shx rm -rf out", "shx rm -rf .next"]
      },
      "configurations": {
        "nodejs": {
          "command": "shx rm -rf node_modules"
        }
      }
    }
  }
}
