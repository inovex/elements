{
  "name": "@inovex.de/elements-angular-builder",
  "version": "8.0.0",
  "description": "This is the angular package of the integration layer for  @inovex.de/elements.",
  "author": "Sven Lindauer <slindauer@inovex.de>",
  "contributors": [
    "Jan-Niklas Vo√ü <jvoss@inovex.de>",
    "Pascal Fecht <pfecht@inovex.de>"
  ],
  "homepage": "https://elements.inovex.de",
  "bugs": {
    "url": "https://github.com/inovex/elements/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "shx rm -rf dist && shx rm -rf elements/dist",
    "lint": "eslint elements/src",
    "lint:fix": "eslint elements/src --fix",
    "e2e": "ng e2e",
    "preinstall": "node ./preinstall.js",
    "postinstall": "node ./postinstall.js",
    "copy:prepare": "shx mkdir -p elements/dist/schematics",
    "copy:collection": "shx cp elements/schematics/collection.json elements/dist/schematics/collection.json",
    "postbuild": "npm run copy:prepare && npm run copy:collection"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.1",
    "@angular/common": "^12.0.1",
    "@angular/compiler": "^12.0.1",
    "@angular/core": "^12.0.1",
    "@angular/forms": "^12.0.1",
    "@angular/platform-browser": "^12.0.1",
    "@angular/platform-browser-dynamic": "^12.0.1",
    "@angular/router": "^12.0.1",
    "@inovex.de/elements": "^8.0.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.1",
    "@angular-eslint/eslint-plugin": "^1.1.0",
    "@angular-eslint/eslint-plugin-template": "^1.1.0",
    "@angular/cli": "^12.0.1",
    "@angular/compiler-cli": "^12.0.1",
    "@angular/language-service": "~12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "codelyzer": "^6.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.22.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.2",
    "protractor": "~7.0.0",
    "shx": "^0.3.2",
    "ts-node": "~7.0.0",
    "tsickle": "^0.39.1",
    "typescript": "~4.2.4"
  },
  "license": "MIT"
}
