{
  "name": "@inovex.de/elements-angular",
  "version": "9.3.1",
  "author": "Sven Lindauer <slindauer@inovex.de>",
  "description": "Angular library for @inovex.de/elements to use the components in Angular projects",
  "contributors": [
    "Jan-Niklas Vo√ü <jvoss@inovex.de>",
    "Pascal Fecht <pfecht@inovex.de>",
    "Sven Lindauer <slindauer@inovex.de>",
    "Benjamin Pagelsdorf <benjamin.pagelsdorf@inovex.de>"
  ],
  "scripts": {
    "build": "node buildHelper.js preBuild && ng-packagr -p ng-package.json -c tsconfig.lib.prod.json && node buildHelper.js sync && pnpm run schematics",
    "start": "node buildHelper.js preBuild && ng-packagr -p ng-package.json -c tsconfig.lib.json --watch",
    "schematics": "tsc -p tsconfig.schematics.json --outDir dist/schematics && shx cp src/schematics/collection.json dist/schematics && shx cp src/schematics/add/schema.json dist/schematics/add",
    "clean": "shx rm -rf dist"
  },
  "homepage": "https://elements.inovex.de",
  "bugs": {
    "url": "https://github.com/inovex/elements/issues"
  },
  "peerDependencies": {
    "@angular/core": ">=14.0.0",
    "@angular/forms": ">=14.0.0",
    "@angular/router": ">=14.0.0",
    "rxjs": ">=7.5.0"
  },
  "dependencies": {
    "@inovex.de/elements": "workspace:*",
    "tslib": "^2.3.0"
  },
  "schematics": "./dist/schematics/collection.json",
  "sideEffects": false,
  "files": [
    "dist/",
    ".yalc"
  ],
  "module": "./dist/fesm2015/inovex.de-elements-angular.mjs",
  "es2020": "./dist/fesm2020/inovex.de-elements-angular.mjs",
  "esm2020": "./dist/esm2020/inovex.de-elements-angular.mjs",
  "fesm2020": "./dist/fesm2020/inovex.de-elements-angular.mjs",
  "fesm2015": "./dist/fesm2015/inovex.de-elements-angular.mjs",
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@angular/compiler": "~15.1.0",
    "@angular/compiler-cli": "~15.1.0",
    "ng-packagr": "~15.1.0",
    "typescript": "~4.9.5"
  },
  "exports": {
    "./package.json": {
      "default": "./dist/package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "esm2020": "./dist/esm2020/inovex.de-elements-angular.mjs",
      "es2020": "./dist/fesm2020/inovex.de-elements-angular.mjs",
      "es2015": "./dist/fesm2015/inovex.de-elements-angular.mjs",
      "node": "./dist/fesm2015/inovex.de-elements-angular.mjs",
      "default": "./dist/fesm2020/inovex.de-elements-angular.mjs"
    }
  }
}
