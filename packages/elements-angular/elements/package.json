{
  "name": "@inovex.de/elements-angular",
  "version": "3.0.1",
  "author": "Sven Lindauer <slindauer@inovex.de>",
  "description": "Angular library for @inovex.de/elements to use the components in Angular projects",
  "contributors": [
    "Pascal Fecht <pfecht@inovex.de>",
    "Sven Lindauer <slindauer@inovex.de>"
  ],
  "homepage": "https://elements.inovex.de",
  "bugs": {
    "url": "https://github.com/inovex/elements/issues"
  },
  "dependencies": {
    "@inovex.de/elements": "^3.0.1"
  },
  "peerDependencies": {
    "@angular/common": "^12.0.1",
    "@angular/core": "^12.0.1"
  },
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "cp --parents schematics/*/schema.json ../out-tsc/lib",
    "copy:files": "cp --parents -p schematics/*/files/** ../out-tsc/lib",
    "copy:collection": "cp schematics/collection.json ../out-tsc/lib/schematics/collection.json",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection",
    "prepublishOnly": "cd ../ && yarn build"
  },
  "publishConfig": {
    "directory": "../dist"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  }
}
