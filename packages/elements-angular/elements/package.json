{
  "name": "@inovex.de/elements-angular",
  "version": "8.0.0",
  "author": "Sven Lindauer <slindauer@inovex.de>",
  "description": "Angular library for @inovex.de/elements to use the components in Angular projects",
  "contributors": [
    "Pascal Fecht <pfecht@inovex.de>",
    "Sven Lindauer <slindauer@inovex.de>"
  ],
  "homepage": "https://elements.inovex.de",
  "bugs": {
    "url": "https://github.com/inovex/elements/issues"
  },
  "dependencies": {
    "@inovex.de/elements": "^8.0.0"
  },
  "peerDependencies": {
    "@angular/common": "^12.0.1",
    "@angular/core": "^12.0.1"
  },
  "scripts": {
    "build": "cd .. && ng build && tsc -p elements/tsconfig.schematics.json",
    "clean": "shx rm -rf dist && shx rm -rf ../dist"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "main": "./dist/bundles/inovex.de-elements-angular.umd.js",
  "module": "./dist/fesm2015/inovex.de-elements-angular.js",
  "es2015": "./dist/fesm2015/inovex.de-elements-angular.js",
  "esm2015": "./dist/esm2015/inovex.de-elements-angular.js",
  "fesm2015": "./dist/fesm2015/inovex.de-elements-angular.js",
  "typings": "./dist/inovex.de-elements-angular.d.ts",
  "sideEffects": false
}
